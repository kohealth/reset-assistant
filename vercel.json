{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "widget.js", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/chat", "dest": "server.js" },
    { "src": "/api/branding", "dest": "server.js" },
    { "src": "/widget.js", "dest": "widget.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}